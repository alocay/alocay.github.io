// CoffeeScript project settings, you can edit it and set custom settings.
{
	// The mappings of source directory and output directory
	"mappings": [
		// {	
		// 	"src": "path/to/source",
		// 	"dest": "path/to/output"
		// }
	],

	// Add the ignore rules that Koala will not search them.
	// e.g. ["*.json", "*.txt", "test", "path/libs"]
	"ignores": ["*.js", "node_modules"],

	// Compile options of CoffeeScript.
	"options": {
		// Compile without a top-level function wrapper.
		"bare": false,

		// Treat stdio as literate style coffee-script.
		"literate": false,

		// generate source map and save as .map files
		"sourceMap": false
	},

	// Other compile options, use the full name of options.
	// e.g, ["--map", ...].
	// Run the command 'sass -h' to see more options.
	"customOptions": []
}